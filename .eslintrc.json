{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "eqeqeq": "off",
        "semi": "off",
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "prefer-const": "off",
        "space-in-parens": "off",
        "react/prop-types": "off",
        "operator-linebreak": "off",
        "multiline-ternary": "off",
        "object-curly-spacing": "off",
        "no-console": "off"
    }
}
